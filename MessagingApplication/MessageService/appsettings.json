{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MongoDB": {
    "ConnectionString": "mongodb://message-db:27017",
    "Database": "Messages"
  },
  "Collections": {
    "Messages": "Messages",
    "Users": "Users"
  },
  "RabbitMQ": {
    "ConnectionString": "amqp://admin:admin@rabbitmq:5672/"
  },
  "Queues": {
    "Users": {
      "Name": "message-service-user-events",
      "Exchange": "users",
      "RoutingKey": "users.#",
      "Events": {
        "Created": "UserCreated"
      }
    }
  }
}