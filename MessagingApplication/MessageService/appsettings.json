{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MongoDB": {
    "ConnectionString": "mongodb://admin:admin@message-db:27017/messageservicedb?authSource=admin",
    "Collections": {
      "Chats": "chats",
      "Messages": "messages",
      "Users": "users"
    }
  },
  "RabbitMQ": {
    "ConnectionString": "amqp://admin:admin@rabbitmq:5672/"
  },
  "Queues": {
    "Users": {
      "Name": "message-service-user-events",
      "Exchange": "users",
      "RoutingKey": "users.#",
      "Events": {
        "Created": "UserCreated",
        "Updated":  "UserUpdated"
      }
    }
  }
}